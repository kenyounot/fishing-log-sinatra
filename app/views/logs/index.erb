    <div class="log-group">
        <h1 class="text-center log-text">Here are your logs</h1>
        <div class="col-md-12 text-center">
            <button class="create-btn btn btn-secondary btn-medium" onclick="location.href='/logs/new'">Create New Log Entry</button>
        </div>
        <div class="log-loop">
            <% @logs.each do |log| %>
                <h3 class="text-center"><a href="/logs/<%= log.id %>"><%= "Date: " + log.date + " " + "Location: " + log.location  %></a></h3>
            <% end %>
        </div>
    </div>
